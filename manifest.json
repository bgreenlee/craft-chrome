{
    "name": "Add Page to Craft",
    "description": "Adds the current page to the end of a document in Craft",
    "version": "1.0.3",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab"],
    "action": {
        "default_title": "Add page to Craft",
        "default_icon": {
          "16": "/images/icon16.png",
          "32": "/images/icon32.png",
          "48": "/images/icon48.png",
          "128": "/images/icon128.png"
        }
    },
    "icons": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon128.png"
    },
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    }     
  }